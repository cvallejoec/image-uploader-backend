{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node-server" }],
  "routes": [
    {
      "src": "/api/uploader",
      "dest": "/index.js",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/api/uploader/:id",
      "dest": "/index.js",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/api/uploader/test/test2",
      "dest": "/index.js",
      "methods": ["GET", "POST"]
    }
  ]
}
